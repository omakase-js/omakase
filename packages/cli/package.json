{
  "name": "@omakase/cli",
  "version": "0.0.6",
  "description": "The Omakase JS CLI",
  "repository": {
    "type": "git",
    "url": "https://github.com/omakase-js/omakase.git"
  },
  "bin": {
    "om": "./bin/run"
  },
  "main": "build/index.js",
  "author": "Orta Therox <orta.therox@gmail.com> & Art.sy Inc",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "build": "tsc -b && cp -R src/generator/templates build/generator",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "release": "release-it",
    "test": "jest",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf build && yarn build && oclif-dev manifest && oclif-dev readme"
  },
  "oclif": {
    "commands": "./build/commands",
    "bin": "om"
  },
  "files": [
    "/bin",
    "/build",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "tslib": "^1",
    "yeoman-environment": "^2.3.4",
    "yeoman-generator": "^3.1.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/dedent": "^0.7.0",
    "@types/graphql": "^14.2.0",
    "@types/yeoman-generator": "^3.0.1",
    "@types/yeoman-test": "^1.7.4",
    "dedent": "^0.7.0",
    "graphql": "^14.3.0",
    "ts-simple-ast": "^20.0.0",
    "yeoman-test": "^1.9.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
